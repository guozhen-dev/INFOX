{
    "manifest_version": 3,
    "name": "Fork Insight",
    "version": "1.0",
    "description": "A Chrome extension version of INFOX with React.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "192": "logo192.png",
        "512": "logo512.png"
      }
    },
    "icons": {
      "192": "logo192.png",
      "512": "logo512.png"
    },
    "permissions": ["activeTab", "storage", "identity"],
    "background": {
      "service_worker": "background.js"
    },
    "key": ""
  }
  