{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A brief description of my Chrome Extension",
    "permissions": [
        "storage",
        "activeTab",
        "identity"
    ],
    "oauth2": {
        "client_id": "a23cbec4da1d5270a30d",
        "client_secret": "fba0b454f15eaf0646a8efa32c6f806b499a76c0",
        "scopes": [
            "user:email"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                {{jsfiles}}
            ]
        }
    ],
    "action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                {{files}}
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "key": ""
}